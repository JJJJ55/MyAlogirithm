package D3;

import java.util.Scanner;

public class Ex6485 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int t=0;t<T;t++) {
			int arr[] = new int[5001];
			int n = sc.nextInt();
			
			for(int i=0;i<n;i++) {
				int a = sc.nextInt();
				int b =sc.nextInt();
				for(int j=a;j<=b;j++) {
					arr[j]++;
				}
			}
			
			int p = sc.nextInt();
			int num[] = new int[p];
			
			for(int i=0;i<num.length;i++) {
				System.out.println(arr[num[i]]);
			}
			
		}
	}
}
