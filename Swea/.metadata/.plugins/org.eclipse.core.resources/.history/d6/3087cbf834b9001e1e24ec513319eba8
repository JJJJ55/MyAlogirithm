package D3;

import java.util.Scanner;

public class Ex6485 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int t=0;t<T;t++) {
			int n = sc.nextInt();
			int [][]arr = new int[n][2];
			for(int i=0;i<n;i++) {
				arr[i][0] = sc.nextInt();
				arr[i][1] = sc.nextInt();
			}
			for(int i=0;i<n;i++) {
				System.out.println(arr[i][0]+" "+arr[i][1]);
			}
		}
	}
}
