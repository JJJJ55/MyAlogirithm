package D2;

import java.util.Scanner;

public class Ex2001 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int t=0;t<T;t++) {
			int n = sc.nextInt();
			int m = sc.nextInt();
			int arr[][] = new int[n][n];
			
			for(int i=0;i<n;i++) {
				for(int j=0;j<n;j++) {
					arr[i][j] = sc.nextInt();
				}
			}
			int max = Integer.MIN_VALUE;
			for(int i=0;i<n-m;i++) {
				for(int j=0;j<n-m;j++) {
					int sum = arr[i][j];
					for(int z=1;z<m;z++) {
						sum+=arr[i+z][j]+arr[i][j+z]+arr[i+z][j+z];
					}
					max = Math.max(max, sum);
				}
			}
			System.out.printf("#%d %d\n",t+1,max);
			
		}
	}
}
