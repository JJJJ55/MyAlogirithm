package D1;

import java.util.Scanner;

public class Ex2070 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int t=0;t<T;t++) {
			int a = sc.nextInt();
			int b = sc.nextInt();
			
			if(a!=b) {
				System.out.printf("#%d %s",t+1,a>b?">":"<");
			} else {
				System.out.println("=");
			}
		}
	}
}
