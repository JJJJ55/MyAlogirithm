package D2;

import java.util.Scanner;

public class Ex1288 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		
		for(int t=0;t<T;t++) {
			int n = sc.nextInt();
			int arr[] = new int[10];
			int idx = 0;
			int i = 2;
			while(true) {
				arr[n]++;
				for(int i=0;i<arr.length;i++) {
					if(arr[i]>0) {
						idx++;
					}
				}
				if(idx==10) {
					
					break;
				} else {
					n= i*n;
					i++;
				}
			}
		}
	}
}
